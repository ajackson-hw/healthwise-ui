{
  "name": "@healthwise-ui/core",
  "version": "0.0.5",
  "description": "Shared React UI library that implements the Healthwise design language.",
  "main": "index.js",
  "unpkg": "healthwise-ui.min.js",
  "keywords": [
    "react",
    "ui",
    "component library",
    "healthwise"
  ],
  "author": "Healthwise",
  "license": "MIT",
  "repository": "healthwise/healthwise-ui",
  "bugs": "https://github.com/healthwise/healthwise-ui/issues",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:iife",
    "build:cjs": "babel --root-mode upward src -d build",
    "build:iife": "rollup -c",
    "prebuild": "npm run clean",
    "postbuild": "node scripts/copyFiles.js",
    "clean": "rimraf build",
    "publish": "npm publish build"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.7",
    "react-aria-menubutton": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  }
}
